mod stdlib::std;
mod compiler::pre;

use std::string::Str;
use std::file::File;

use pre::abyss_lexer::cursor::Cursor;
use pre::abyss_lexer::scanner::Scanner;
use pre::abyss_lexer::token::token_from;
use pre::abyss_lexer::token::KindEof;


fn app_main {


    let code = File.read_to_string("test.a")

    let s = Scanner.new(code);

    
    forever {
        let a = s.next_raw()
        a.get_kind().println()


        if a.kind is KindEof {
            out
        }
        
    }

}



