struct FILE;

fn fopen(filename: const &char, mode: const &char): &FILE;
fn fclose(stream: &FILE): i32;
fn fread(ptr: &pass, s: usize, n: usize, stream: &FILE): usize;
fn fwrite(ptr: const &pass, s: usize, n: usize, stream: &FILE): usize;
fn fgets(st: &char, s: i32, stream: &FILE): &char;
fn fputs(s: const &char, stream: &FILE): i32;
fn fseek(stream: &FILE, offset: i64, whence: i32): i32;
fn ftell(stream: &FILE): i64;
fn rewind(stream: &FILE);


fn memset(s: &pass, c: i32, n: usize): &pass;
fn memcpy(dest: &pass, src: &pass, n: i64): &pass;
fn malloc(s: i64): &pass                as c_malloc;
fn realloc(ptr: &pass, s: i64): &pass   as c_realloc;
fn free(ptr: &pass)                     as c_free;
fn exit(status: i64);
fn printf(s: const &char, ..): i32;
fn getchar(): i32;
fn scanf(fmt: const &char, ..): i32;
fn atoll(s: const &char): i64;
fn atof(s: const &char): f64;  

fn print(s: &u8, v: i64);


