struct Rng {
    _state: i64
}

fn seed(s: i64): Rng {
    if s == 0 {
        ret struct Rng { _state: 1 }
    }
    ret struct Rng { _state: s }
}


impl Rng {
    fn next_i64(self: &Rng): i64 {
        let x: i64 = self._state;

        x = x ^ (x << 13);
        x = x ^ (x >> 7);
        x = x ^ (x << 17);

        self._state = x;
        ret x;
    }

